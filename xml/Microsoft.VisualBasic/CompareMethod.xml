      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.VisualBasic.CompareMethod">
          <source>Indicates how to compare strings when calling comparison functions.</source>
          <target state="translated">Gibt in Aufrufen von Vergleichsfunktionen an, wie Zeichenfolgen miteinander verglichen werden sollen.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.VisualBasic.CompareMethod">
          <source>When you call comparison functions, you can use the <ph id="ph1">`CompareMethod`</ph> enumeration in your code in place of the actual values.</source>
          <target state="translated">Beim Aufrufen von Vergleichsfunktionen können Sie die <ph id="ph1">`CompareMethod`</ph> Enumeration in Ihrem Code anstelle der eigentlichen Werte.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.VisualBasic.CompareMethod">
          <source>The <ph id="ph1">`Compare`</ph> argument takes the <ph id="ph2">`CompareMethod`</ph> enumeration members.</source>
          <target state="translated">Die <ph id="ph1">`Compare`</ph> Argument übernimmt die <ph id="ph2">`CompareMethod`</ph> Enumerationsmember.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.VisualBasic.CompareMethod">
          <source>For details on binary and text comparisons, see <bpt id="p1">[</bpt>Option Compare Statement<ept id="p1">](~/docs/visual-basic/language-reference/statements/option-compare-statement.md)</ept>.</source>
          <target state="translated">Ausführliche Informationen zum Vergleichen von Binär- und Textmodus finden Sie unter <bpt id="p1">[</bpt>Option Compare-Anweisung<ept id="p1">](~/docs/visual-basic/language-reference/statements/option-compare-statement.md)</ept>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:Microsoft.VisualBasic.CompareMethod.Binary">
          <source>Performs a binary comparison.</source>
          <target state="translated">Führt einen binären Vergleich durch.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:Microsoft.VisualBasic.CompareMethod.Binary">
          <source>This member is equivalent to the Visual Basic constant <ph id="ph1">&lt;see langword="vbBinaryCompare" /&gt;</ph>.</source>
          <target state="translated">Dieser Member ist identisch mit der Visual Basic-Konstanten <ph id="ph1">&lt;see langword="vbBinaryCompare" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:Microsoft.VisualBasic.CompareMethod.Text">
          <source>Performs a textual comparison.</source>
          <target state="translated">Führt einen Textvergleich durch.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:Microsoft.VisualBasic.CompareMethod.Text">
          <source>This member is equivalent to the Visual Basic constant <ph id="ph1">&lt;see langword="vbTextCompare" /&gt;</ph>.</source>
          <target state="translated">Dieser Member ist identisch mit der Visual Basic-Konstanten <ph id="ph1">&lt;see langword="vbTextCompare" /&gt;</ph>.</target>       </trans-unit>
