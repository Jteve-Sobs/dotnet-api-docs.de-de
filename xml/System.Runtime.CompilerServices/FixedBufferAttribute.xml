      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Runtime.CompilerServices.FixedBufferAttribute">
          <source>Indicates that a field should be treated as containing a fixed number of elements of the specified primitive type.</source>
          <target state="translated">Gibt an, dass ein Feld als Feld mit einer festen Anzahl von Elementen des angegebenen primitiven Typs behandelt werden soll.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Runtime.CompilerServices.FixedBufferAttribute">
          <source>This class cannot be inherited.</source>
          <target state="translated">Diese Klasse kann nicht vererbt werden.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Runtime.CompilerServices.FixedBufferAttribute">
          <source>Apply the <ph id="ph1">&lt;xref:System.Runtime.CompilerServices.FixedBufferAttribute&gt;</ph> attribute to a field to indicate that it should be treated as a fixed-length inline array.</source>
          <target state="translated">Anwenden der <ph id="ph1">&lt;xref:System.Runtime.CompilerServices.FixedBufferAttribute&gt;</ph> -Attribut auf ein Feld, um anzugeben, dass es als ein Inlinearray fester Länge behandelt werden sollen.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Runtime.CompilerServices.FixedBufferAttribute">
          <source>The type of the field that the attribute is applied to must have a size that can contain the indicated number of elements.</source>
          <target state="translated">Der Typ des Felds, das das Attribut angewendet wird, benötigen eine Größe, die die angegebene Anzahl von Elementen enthalten kann.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Runtime.CompilerServices.FixedBufferAttribute.#ctor(System.Type,System.Int32)">
          <source>The type of the elements contained in the buffer.</source>
          <target state="translated">Der Typ der im Puffer enthaltenen Elemente.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Runtime.CompilerServices.FixedBufferAttribute.#ctor(System.Type,System.Int32)">
          <source>The number of elements in the buffer.</source>
          <target state="translated">Die Anzahl der Elemente im Puffer.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Runtime.CompilerServices.FixedBufferAttribute.#ctor(System.Type,System.Int32)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Runtime.CompilerServices.FixedBufferAttribute" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Runtime.CompilerServices.FixedBufferAttribute" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Runtime.CompilerServices.FixedBufferAttribute.ElementType">
          <source>Gets the type of the elements contained in the fixed buffer.</source>
          <target state="translated">Ruft den Typ der im Puffer mit fester Größe enthaltenen Elemente ab.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.CompilerServices.FixedBufferAttribute.ElementType">
          <source>The type of the elements.</source>
          <target state="translated">Der Typ der Elemente.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Runtime.CompilerServices.FixedBufferAttribute.Length">
          <source>Gets the number of elements in the fixed buffer.</source>
          <target state="translated">Ruft die Anzahl der Elemente im Puffer mit fester Größe ab.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.CompilerServices.FixedBufferAttribute.Length">
          <source>The number of elements in the fixed buffer.</source>
          <target state="translated">Die Anzahl der Elemente im Puffer mit fester Größe.</target>       </trans-unit>
