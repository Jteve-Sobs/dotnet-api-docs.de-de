      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.EntityClient.EntityTransaction">
          <source>Specifies the transaction for an <ph id="ph1">&lt;see cref="P:System.Data.EntityClient.EntityCommand" /&gt;</ph>.</source>
          <target state="translated">Gibt die Transaktion für einen <ph id="ph1">&lt;see cref="P:System.Data.EntityClient.EntityCommand" /&gt;</ph> an.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Data.EntityClient.EntityTransaction">
          <source>The <ph id="ph1">&lt;xref:System.Data.EntityClient.EntityTransaction&gt;</ph> is obtained from the <ph id="ph2">&lt;xref:System.Data.EntityClient.EntityConnection&gt;</ph> and specifies one or more <ph id="ph3">&lt;xref:System.Data.EntityClient.EntityCommand&gt;</ph> objects.</source>
          <target state="translated">Die <ph id="ph1">&lt;xref:System.Data.EntityClient.EntityTransaction&gt;</ph> wird über die <ph id="ph2">&lt;xref:System.Data.EntityClient.EntityConnection&gt;</ph> abgerufen und legt mindestens ein <ph id="ph3">&lt;xref:System.Data.EntityClient.EntityCommand&gt;</ph>-Objekt fest.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Data.EntityClient.EntityTransaction">
          <source>The <ph id="ph1">&lt;xref:System.Data.EntityClient.EntityTransaction&gt;</ph> wraps the transaction of the underlying data provider's transaction object.</source>
          <target state="translated">Die <ph id="ph1">&lt;xref:System.Data.EntityClient.EntityTransaction&gt;</ph> bindet als Wrapper die Transaktion des Transaktionsobjekts des zugrunde liegenden Datenanbieters ein.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Data.EntityClient.EntityTransaction">
          <source>For code examples, see <bpt id="p1">[</bpt>Managing Connections and Transactions<ept id="p1">](http://msdn.microsoft.com/library/6d694aa9-b290-4738-ba8e-3ed4e5b148f9)</ept>.</source>
          <target state="translated">Codebeispiele finden Sie unter <bpt id="p1">[</bpt>Verwalten von Verbindungen und Transaktionen<ept id="p1">](http://msdn.microsoft.com/library/6d694aa9-b290-4738-ba8e-3ed4e5b148f9)</ept>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Data.EntityClient.EntityTransaction.Commit">
          <source>Commits the underlying transaction.</source>
          <target state="translated">Führt einen Commit der zugrunde liegenden Transaktion aus.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Data.EntityClient.EntityTransaction.Connection">
          <source>Gets <ph id="ph1">&lt;see cref="T:System.Data.EntityClient.EntityConnection" /&gt;</ph> for this <ph id="ph2">&lt;see cref="T:System.Data.EntityClient.EntityTransaction" /&gt;</ph>.</source>
          <target state="translated">Ruft die <ph id="ph1">&lt;see cref="T:System.Data.EntityClient.EntityConnection" /&gt;</ph> für diesen <ph id="ph2">&lt;see cref="T:System.Data.EntityClient.EntityTransaction" /&gt;</ph> ab.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.EntityClient.EntityTransaction.Connection">
          <source>An <ph id="ph1">&lt;see cref="T:System.Data.EntityClient.EntityConnection" /&gt;</ph> to the underlying data source.</source>
          <target state="translated">Eine <ph id="ph1">&lt;see cref="T:System.Data.EntityClient.EntityConnection" /&gt;</ph> der zugrunde liegenden Datenquelle.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Data.EntityClient.EntityTransaction.IsolationLevel">
          <source>Gets the isolation level of this <ph id="ph1">&lt;see cref="T:System.Data.EntityClient.EntityTransaction" /&gt;</ph>.</source>
          <target state="translated">Ruft die Isolationsstufe dieser <ph id="ph1">&lt;see cref="T:System.Data.EntityClient.EntityTransaction" /&gt;</ph> ab.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.EntityClient.EntityTransaction.IsolationLevel">
          <source>An <ph id="ph1">&lt;see cref="T:System.Data.IsolationLevel" /&gt;</ph> enumeration value that represents the isolation level of the underlying transaction.</source>
          <target state="translated">Ein <ph id="ph1">&lt;see cref="T:System.Data.IsolationLevel" /&gt;</ph>-Enumerationswert, der die Isolationsstufe der zugrunde liegenden Transaktion darstellt.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Data.EntityClient.EntityTransaction.Rollback">
          <source>Rolls back the underlying transaction.</source>
          <target state="translated">Führt einen Rollback für die zugrunde liegende Transaktion aus.</target>       </trans-unit>
