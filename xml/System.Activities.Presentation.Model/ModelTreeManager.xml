        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.Model.ModelTreeManager.GetModelItem(System.Object,System.Boolean)">
          <source>Returns the model item for the manager.</source>
          <target state="translated">Gibt das Modellelement für den Manager zurück.</target>       </trans-unit>
