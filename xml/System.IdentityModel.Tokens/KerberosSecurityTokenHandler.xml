      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.IdentityModel.Tokens.KerberosSecurityTokenHandler">
          <source>Represents a security token handler that processes Kerberos tokens.</source>
          <target state="translated">Stellt einen Sicherheitstokenhandler dar, der Kerberos-Token verarbeitet.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.IdentityModel.Tokens.KerberosSecurityTokenHandler">
          <source>Handles tokens of type <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph>.</source>
          <target state="translated">Behandelt das Token des Typs <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosSecurityTokenHandler" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosSecurityTokenHandler" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.CanValidateToken">
          <source>Gets a value that indicates whether the current instance can validate security tokens.</source>
          <target state="translated">Ruft einen Wert ab, der angibt, ob die aktuelle Instanz Sicherheitstoken überprüfen kann.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.CanValidateToken">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the handler can validate tokens; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph>, wenn der Handler Token überprüfen kann; andernfalls <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.CanValidateToken">
          <source>The default is <ph id="ph1">&lt;see langword="true" /&gt;</ph>.</source>
          <target state="translated">Der Standardwert ist <ph id="ph1">&lt;see langword="true" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.CanValidateToken">
          <source>Indicates whether the handler can validate tokens of the type specified by the <ph id="ph1">&lt;xref:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.TokenType%2A&gt;</ph> property.</source>
          <target state="translated">Gibt an, ob der Handler des Typs vom angegebenen Token überprüfen kann die <ph id="ph1">&lt;xref:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.TokenType%2A&gt;</ph> Eigenschaft.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.GetTokenTypeIdentifiers">
          <source>Gets the Kerberos Security token type defined in WS-Security Kerberos Security Token profile.</source>
          <target state="translated">Ruft den Typ des Kerberos-Sicherheitstokens ab, der im WS-Security-Kerberos-Sicherheitstoken-Profil definiert ist.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.GetTokenTypeIdentifiers">
          <source>The Kerberos Security token type URI.</source>
          <target state="translated">Der URI des Kerberos-Sicherheitstokentyps.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.GetTokenTypeIdentifiers">
          <source>By default, the value of the <ph id="ph1">&lt;see cref="P:System.IdentityModel.Tokens.SecurityTokenTypes.Kerberos" /&gt;</ph> property.</source>
          <target state="translated">Der standardmäßige Werts der <ph id="ph1">&lt;see cref="P:System.IdentityModel.Tokens.SecurityTokenTypes.Kerberos" /&gt;</ph>-Eigenschaft.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.TokenType">
          <source>Gets the type of the token handled by the current instance.</source>
          <target state="translated">Ruft den Typ des Tokens ab, das durch die aktuelle Instanz verarbeitet wird.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.TokenType">
          <source>The type of the token handled by the current instance.</source>
          <target state="translated">Der Typ des Tokens, das durch die aktuelle Instanz verarbeitet wird.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.TokenType">
          <source>By default this is the type of the <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph> class.</source>
          <target state="translated">Standardmäßig ist dies der Typ der <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.ValidateToken(System.IdentityModel.Tokens.SecurityToken)">
          <source>The security token to be validated.</source>
          <target state="translated">Das zu überprüfende Sicherheitstoken.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.ValidateToken(System.IdentityModel.Tokens.SecurityToken)">
          <source>This must be an instance of the <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph> class.</source>
          <target state="translated">Dabei muss es sich um eine Instanz der <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph>-Klasse handeln.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.ValidateToken(System.IdentityModel.Tokens.SecurityToken)">
          <source>Validates the specified Kerberos security token.</source>
          <target state="translated">Überprüft das angegebene Kerberos-Sicherheitstoken.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.ValidateToken(System.IdentityModel.Tokens.SecurityToken)">
          <source>The identities that are contained in the token.</source>
          <target state="translated">Die im Token enthaltenen Identitäten.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.ValidateToken(System.IdentityModel.Tokens.SecurityToken)">
          <source><ph id="ph1">&lt;paramref name="token" /&gt;</ph> is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="token" /&gt;</ph> ist <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="M:System.IdentityModel.Tokens.KerberosSecurityTokenHandler.ValidateToken(System.IdentityModel.Tokens.SecurityToken)">
          <source>The specified token is not assignable as a <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph>.</source>
          <target state="translated">Das angegebene Token ist nicht als <ph id="ph1">&lt;see cref="T:System.IdentityModel.Tokens.KerberosReceiverSecurityToken" /&gt;</ph> zuweisbar.</target>       </trans-unit>
