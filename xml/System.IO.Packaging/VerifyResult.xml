      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.IO.Packaging.VerifyResult">
          <source>Specifies the result of a certificate or signature verification.</source>
          <target state="translated">Gibt das Ergebnis einer Zertifikat- oder Signaturüberprüfung an.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.IO.Packaging.VerifyResult">
          <source>The following example shows how to use the <ph id="ph1">&lt;xref:System.IO.Packaging.VerifyResult&gt;</ph> enumeration.</source>
          <target state="translated">Das folgende Beispiel zeigt, wie Sie die <ph id="ph1">&lt;xref:System.IO.Packaging.VerifyResult&gt;</ph> Enumeration.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.IO.Packaging.VerifyResult">
          <source>For the complete sample, see <bpt id="p1">[</bpt>Creating a Package with a Digital Signature Sample<ept id="p1">](http://go.microsoft.com/fwlink/?LinkID=159966)</ept>.</source>
          <target state="translated">Das vollständige Beispiel finden Sie unter <bpt id="p1">[</bpt>Erstellen eines Pakets mit einer digitalen Signatur<ept id="p1">](http://go.microsoft.com/fwlink/?LinkID=159966)</ept>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.IO.Packaging.VerifyResult.CertificateRequired">
          <source>The X.509 certificate is not available to verify the signature.</source>
          <target state="translated">Das X.509-Zertifikat ist nicht zum Überprüfen der Signatur verfügbar.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.IO.Packaging.VerifyResult.InvalidCertificate">
          <source>The X.509 certificate is not valid.</source>
          <target state="translated">Das X.509-Zertifikat ist ungültig.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.IO.Packaging.VerifyResult.InvalidSignature">
          <source>The signature is not valid.</source>
          <target state="translated">Die Signatur ist ungültig.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.IO.Packaging.VerifyResult.NotSigned">
          <source>The specified package or part has no signature.</source>
          <target state="translated">Der angegebene Paket oder Teil hat keine Signatur.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.IO.Packaging.VerifyResult.ReferenceNotFound">
          <source>A reference relationship to the signature was not found.</source>
          <target state="translated">Es wurde keine Verweisbeziehung zu der Signatur gefunden.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.IO.Packaging.VerifyResult.Success">
          <source>The verification was successful.</source>
          <target state="translated">Die Überprüfung war erfolgreich.</target>       </trans-unit>
