      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.ComIntegration.DllHostInitializer">
          <source>This class provides the implementation of the <ph id="ph1">&lt;see cref="T:System.EnterpriseServices.IProcessInitializer" /&gt;</ph> interface for the initialization of Windows Communication Foundation (WCF) COM+ integration services.</source>
          <target state="translated">Diese Klasse stellt die Implementierung der <ph id="ph1">&lt;see cref="T:System.EnterpriseServices.IProcessInitializer" /&gt;</ph>-Schnittstelle für die Initialisierung von Windows Communication Foundation COM+ Integration Services bereit.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.ComIntegration.DllHostInitializer">
          <source>It should not be used directly.</source>
          <target state="translated">Sie sollte nicht direkt verwendet werden.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.ComIntegration.DllHostInitializer.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.ComIntegration.DllHostInitializer" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.ServiceModel.ComIntegration.DllHostInitializer" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.ComIntegration.DllHostInitializer.Shutdown">
          <source>Shuts down all the COM+ service hosts that were started by calls to the <ph id="ph1">&lt;see langword="Startup" /&gt;</ph> method.</source>
          <target state="translated">Beendet alle COM+-Diensthosts, die durch Aufrufe der <ph id="ph1">&lt;see langword="Startup" /&gt;</ph>-Methode gestartet wurden.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.ComIntegration.DllHostInitializer.Startup(System.Object)">
          <source>Always pass <ph id="ph1">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="null" /&gt;</ph> immer übergeben.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.ComIntegration.DllHostInitializer.Startup(System.Object)">
          <source>Starts a COM+ service host for the current COM+ application.</source>
          <target state="translated">Startet einen COM+-Diensthost für die aktuelle COM+-Anwendung.</target>       </trans-unit>
