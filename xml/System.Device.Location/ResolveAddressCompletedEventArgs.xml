      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Device.Location.ResolveAddressCompletedEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:System.Device.Location.ICivicAddressResolver.ResolveAddressCompleted" /&gt;</ph> event.</source>
          <target state="translated">Stellt Daten für das <ph id="ph1">&lt;see cref="E:System.Device.Location.ICivicAddressResolver.ResolveAddressCompleted" /&gt;</ph>-Ereignis bereit.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Device.Location.ResolveAddressCompletedEventArgs">
          <source>The following example shows how to use the <ph id="ph1">&lt;xref:System.Device.Location.ResolveAddressCompletedEventArgs&gt;</ph>.</source>
          <target state="translated">Das folgende Beispiel zeigt, wie Sie die <ph id="ph1">&lt;xref:System.Device.Location.ResolveAddressCompletedEventArgs&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Device.Location.ResolveAddressCompletedEventArgs.#ctor(System.Device.Location.CivicAddress,System.Exception,System.Boolean,System.Object)">
          <source>The <ph id="ph1">&lt;see cref="T:System.Device.Location.CivicAddress" /&gt;</ph> that contains the address resolved from the latitude/longitude location, if successful.</source>
          <target state="translated">Das <ph id="ph1">&lt;see cref="T:System.Device.Location.CivicAddress" /&gt;</ph>-Objekt, das die aus der Breiten-/Längenposition aufgelöste Adresse enthält (sofern die Position erfolgreich aufgelöst wurde).</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Device.Location.ResolveAddressCompletedEventArgs.#ctor(System.Device.Location.CivicAddress,System.Exception,System.Boolean,System.Object)">
          <source>The exception object for any exception that may have occurred during the attempt to resolve the address.</source>
          <target state="translated">Das Ausnahmeobjekt für jede Ausnahme, die beim Auflösen der Adresse auftreten kann.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Device.Location.ResolveAddressCompletedEventArgs.#ctor(System.Device.Location.CivicAddress,System.Exception,System.Boolean,System.Object)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the operation was cancelled; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph>, wenn der Vorgang abgebrochen wurde, andernfalls <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Device.Location.ResolveAddressCompletedEventArgs.#ctor(System.Device.Location.CivicAddress,System.Exception,System.Boolean,System.Object)">
          <source>A token for tracking the request to resolve the address.</source>
          <target state="translated">Ein Token zum Verfolgen der Anforderung zum Auflösen der Adresse.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Device.Location.ResolveAddressCompletedEventArgs.#ctor(System.Device.Location.CivicAddress,System.Exception,System.Boolean,System.Object)">
          <source>May be <ph id="ph1">&lt;see langword="null" /&gt;</ph> if not used.</source>
          <target state="translated">Kann <ph id="ph1">&lt;see langword="null" /&gt;</ph> sein, wenn es nicht verwendet wird.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Device.Location.ResolveAddressCompletedEventArgs.#ctor(System.Device.Location.CivicAddress,System.Exception,System.Boolean,System.Object)">
          <source>Initializes a new instance of <ph id="ph1">&lt;see cref="T:System.Device.Location.ResolveAddressCompletedEventArgs" /&gt;</ph>.</source>
          <target state="translated">Initialisiert eine neue Instanz von <ph id="ph1">&lt;see cref="T:System.Device.Location.ResolveAddressCompletedEventArgs" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Device.Location.ResolveAddressCompletedEventArgs.#ctor(System.Device.Location.CivicAddress,System.Exception,System.Boolean,System.Object)">
          <source>This constructor is public so that classes implementing the <ph id="ph1">&lt;xref:System.Device.Location.ICivicAddressResolver&gt;</ph> interface can create this object to fire events.</source>
          <target state="translated">Dieser Konstruktor ist öffentlich, so, dass implementieren Klassen die <ph id="ph1">&lt;xref:System.Device.Location.ICivicAddressResolver&gt;</ph> Schnittstelle dieses Objekt zum Auslösen von Ereignissen erstellen kann.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Device.Location.ResolveAddressCompletedEventArgs.Address">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Device.Location.CivicAddress" /&gt;</ph> associated with the <ph id="ph2">&lt;see cref="E:System.Device.Location.ICivicAddressResolver.ResolveAddressCompleted" /&gt;</ph> event.</source>
          <target state="translated">Ruft das <ph id="ph1">&lt;see cref="T:System.Device.Location.CivicAddress" /&gt;</ph> ab, das dem <ph id="ph2">&lt;see cref="E:System.Device.Location.ICivicAddressResolver.ResolveAddressCompleted" /&gt;</ph>-Ereignis zugeordnet ist.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Device.Location.ResolveAddressCompletedEventArgs.Address">
          <source>The <ph id="ph1">&lt;see cref="T:System.Device.Location.CivicAddress" /&gt;</ph> associated with the <ph id="ph2">&lt;see cref="E:System.Device.Location.ICivicAddressResolver.ResolveAddressCompleted" /&gt;</ph> event.</source>
          <target state="translated">Das <ph id="ph1">&lt;see cref="T:System.Device.Location.CivicAddress" /&gt;</ph>, das dem <ph id="ph2">&lt;see cref="E:System.Device.Location.ICivicAddressResolver.ResolveAddressCompleted" /&gt;</ph>-Ereignis zugeordnet ist.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Device.Location.ResolveAddressCompletedEventArgs.Address">
          <source>If the address could not be resolved, this property returns <ph id="ph1">&lt;see cref="F:System.Device.Location.CivicAddress.Unknown" /&gt;</ph>.</source>
          <target state="translated">Wenn die Adresse nicht aufgelöst werden konnte, gibt diese Eigenschaft <ph id="ph1">&lt;see cref="F:System.Device.Location.CivicAddress.Unknown" /&gt;</ph> zurück.</target>       </trans-unit>
