        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute">
          <source>Specifies which types a <ph id="ph1">&lt;see cref="T:System.Windows.Forms.ToolStripItem" /&gt;</ph> can appear in.</source>
          <target state="translated">Gibt an, in welchen Typen ein <ph id="ph1">&lt;see cref="T:System.Windows.Forms.ToolStripItem" /&gt;</ph> angezeigt werden kann.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute">
          <source>This class cannot be inherited.</source>
          <target state="translated">Diese Klasse kann nicht vererbt werden.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute">
          <source>Available types are <ph id="ph1">&lt;xref:System.Windows.Forms.ToolStrip&gt;</ph>, <ph id="ph2">&lt;xref:System.Windows.Forms.MenuStrip&gt;</ph>, <ph id="ph3">&lt;xref:System.Windows.Forms.ContextMenuStrip&gt;</ph>, and <ph id="ph4">&lt;xref:System.Windows.Forms.StatusStrip&gt;</ph>.</source>
          <target state="translated">Die verfügbaren Typen sind <ph id="ph1">&lt;xref:System.Windows.Forms.ToolStrip&gt;</ph>, <ph id="ph2">&lt;xref:System.Windows.Forms.MenuStrip&gt;</ph>, <ph id="ph3">&lt;xref:System.Windows.Forms.ContextMenuStrip&gt;</ph>, und <ph id="ph4">&lt;xref:System.Windows.Forms.StatusStrip&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.#ctor">
          <source>Initializes a new default instance of the <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Standardinstanz der <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.#ctor(System.Windows.Forms.Design.ToolStripItemDesignerAvailability)">
          <source>A <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailability" /&gt;</ph> value specifying the visibility.</source>
          <target state="translated">Ein <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailability" /&gt;</ph>-Wert, der die Sichtbarkeit angibt.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.#ctor(System.Windows.Forms.Design.ToolStripItemDesignerAvailability)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph> class with the specified visibility.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph>-Klasse mit der angegebenen Sichtbarkeit.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.Default">
          <source>Specifies the default value of the <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph>.</source>
          <target state="translated">Gibt den Standardwert des <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute" /&gt;</ph> an.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.Default">
          <source>This field is read-only.</source>
          <target state="translated">Dieses Feld ist schreibgeschützt.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.Equals(System.Object)">
          <source>An <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> to compare with this instance or a null reference (<ph id="ph2">&lt;see langword="Nothing" /&gt;</ph> in Visual Basic).</source>
          <target state="translated">Ein <ph id="ph1">&lt;see cref="T:System.Object" /&gt;</ph> für den Vergleich mit dieser Instanz oder ein Nullverweis (<ph id="ph2">&lt;see langword="Nothing" /&gt;</ph> in Visual Basic).</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.Equals(System.Object)">
          <source>Returns a value that indicates whether this instance is equal to a specified object.</source>
          <target state="translated">Gibt einen Wert zurück, der angibt, ob diese Instanz gleich einem angegebenen Objekt ist.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.Equals(System.Object)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if <ph id="ph2">&lt;paramref name="obj" /&gt;</ph> equals the type and value of this instance; otherwise, <ph id="ph3">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph>, wenn <ph id="ph2">&lt;paramref name="obj" /&gt;</ph> dem Typ und dem Wert dieser Instanz entspricht, andernfalls <ph id="ph3">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.GetHashCode">
          <source>Returns the hash code for this instance.</source>
          <target state="translated">Gibt den Hashcode für diese Instanz zurück.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.GetHashCode">
          <source>A 32-bit signed integer hash code.</source>
          <target state="translated">Ein 32-Bit-Hashcode als ganze Zahl mit Vorzeichen.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.IsDefaultAttribute">
          <source>When overriden in a derived class, indicates whether the value of this instance is the default value for the derived class.</source>
          <target state="translated">Beim Überschreiben in einer abgeleiteten Klasse wird angegeben, ob der Wert dieser Instanz der Standardwert für die abgeleitete Klasse ist.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.IsDefaultAttribute">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if this instance is the default attribute for the class; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph>, wenn diese Instanz das Standardattribut für die Klasse ist, andernfalls <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="P:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.ItemAdditionVisibility">
          <source>Gets the visibility of a <ph id="ph1">&lt;see cref="T:System.Windows.Forms.ToolStripItem" /&gt;</ph>.</source>
          <target state="translated">Ruft die Sichtbarkeit eines <ph id="ph1">&lt;see cref="T:System.Windows.Forms.ToolStripItem" /&gt;</ph> ab.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Forms.Design.ToolStripItemDesignerAvailabilityAttribute.ItemAdditionVisibility">
          <source>A <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailability" /&gt;</ph> representing the visibility.</source>
          <target state="translated">Eine <ph id="ph1">&lt;see cref="T:System.Windows.Forms.Design.ToolStripItemDesignerAvailability" /&gt;</ph>, die die Sichtbarkeit darstellt.</target>       </trans-unit>
