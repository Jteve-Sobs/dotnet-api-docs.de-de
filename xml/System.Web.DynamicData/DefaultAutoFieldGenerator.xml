        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.DynamicData.DefaultAutoFieldGenerator">
          <source>Generates dynamic data fields (columns) for data-bound controls automatically.</source>
          <target state="translated">Generiert automatisch dynamische Datenfelder (Spalten) für datengebundene Steuerelemente.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.DynamicData.DefaultAutoFieldGenerator">
