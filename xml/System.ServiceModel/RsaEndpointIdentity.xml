        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.RsaEndpointIdentity.#ctor(System.Security.Cryptography.X509Certificates.X509Certificate2)">
          <source>The <ph id="ph1">&lt;paramref name="cert" /&gt;</ph> public key is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated">Der öffentliche Schlüssel von <ph id="ph1">&lt;paramref name="cert" /&gt;</ph> ist <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.RsaEndpointIdentity.#ctor(System.Security.Cryptography.X509Certificates.X509Certificate2)">
