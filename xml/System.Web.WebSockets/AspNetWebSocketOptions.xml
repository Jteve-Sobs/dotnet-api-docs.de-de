      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.WebSockets.AspNetWebSocketOptions">
          <source>Specifies configuration settings for an <ph id="ph1">&lt;see cref="T:System.Web.WebSockets.AspNetWebSocket" /&gt;</ph> connection.</source>
          <target state="translated">Gibt Konfigurationseinstellungen für eine <ph id="ph1">&lt;see cref="T:System.Web.WebSockets.AspNetWebSocket" /&gt;</ph>-Verbindung an.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Web.WebSockets.AspNetWebSocketOptions.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.WebSockets.AspNetWebSocketOptions" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Web.WebSockets.AspNetWebSocketOptions" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Web.WebSockets.AspNetWebSocketOptions.RequireSameOrigin">
          <source>Gets or sets whether the URL that initiatedthe WebSocket connection corresponds to the current server.</source>
          <target state="translated">Ruft ab oder legt fest, ob die URL, die die WebSocket-Verbindung initiiert hat, dem aktuellen Server entspricht.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.WebSockets.AspNetWebSocketOptions.RequireSameOrigin">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the URL that initiatedthe WebSocket connection corresponds to the current server; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> wenn die URL, die die WebSocket-Verbindung initiiert hat, dem aktuellen Server entspricht; andernfalls <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Web.WebSockets.AspNetWebSocketOptions.SubProtocol">
          <source>Gets or sets the name of an application-specific protocol that a remote client and a server can use to exchange data over an <ph id="ph1">&lt;see cref="T:System.Web.WebSockets.AspNetWebSocket" /&gt;</ph> connection.</source>
          <target state="translated">Ruft den Namen eines anwendungsspezifischen Protokolls ab, das ein Remote Client und ein Server nutzen können, um Daten mit einer <ph id="ph1">&lt;see cref="T:System.Web.WebSockets.AspNetWebSocket" /&gt;</ph> Verbindung austauschen zu können, oder legt diesen fest.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.WebSockets.AspNetWebSocketOptions.SubProtocol">
          <source>The name of the protocol.</source>
          <target state="translated">Der Name des Protokolls.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Web.WebSockets.AspNetWebSocketOptions.SubProtocol">
          <source>The protocol name assigned to the property contains invalid characters.</source>
          <target state="translated">Der der Eigenschaft zugewiesene Protokollname enthält ungültige Zeichen.</target>       </trans-unit>
