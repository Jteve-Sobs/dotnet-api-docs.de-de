      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.EnterpriseServices.SecurityIdentity">
          <source>Contains information that regards an identity in a COM+ call chain.</source>
          <target state="translated">Enthält Informationen über eine Identität in einer COM+-Aufrufkette.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.EnterpriseServices.SecurityIdentity">
          <source>You do not create new instances programmatically.</source>
          <target state="translated">Neue Instanzen werden nicht programmgesteuert erstellt werden.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.EnterpriseServices.SecurityIdentity">
          <source>Instead, you obtain new instances from <ph id="ph1">&lt;xref:System.EnterpriseServices.SecurityCallContext&gt;</ph> and <ph id="ph2">&lt;xref:System.EnterpriseServices.SecurityCallers&gt;</ph>.</source>
          <target state="translated">Stattdessen erstellen Sie neue Instanzen von <ph id="ph1">&lt;xref:System.EnterpriseServices.SecurityCallContext&gt;</ph> und <ph id="ph2">&lt;xref:System.EnterpriseServices.SecurityCallers&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.EnterpriseServices.SecurityIdentity.AccountName">
          <source>Gets the name of the user described by this identity.</source>
          <target state="translated">Ruft den Namen des Benutzers ab, der durch diese Identität beschrieben wird.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.EnterpriseServices.SecurityIdentity.AccountName">
          <source>The name of the user described by this identity.</source>
          <target state="translated">Der Name des Benutzers, der durch diese Identität beschrieben wird.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.EnterpriseServices.SecurityIdentity.AuthenticationLevel">
          <source>Gets the authentication level of the user described by this identity.</source>
          <target state="translated">Ruft die Authentifizierungsebene des Benutzers ab, der durch diese Identität beschrieben wird.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.EnterpriseServices.SecurityIdentity.AuthenticationLevel">
          <source>One of the <ph id="ph1">&lt;see cref="T:System.EnterpriseServices.AuthenticationOption" /&gt;</ph> values.</source>
          <target state="translated">Einer der <ph id="ph1">&lt;see cref="T:System.EnterpriseServices.AuthenticationOption" /&gt;</ph>-Werte.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.EnterpriseServices.SecurityIdentity.AuthenticationService">
          <source>Gets the authentication service described by this identity.</source>
          <target state="translated">Ruft den Authentifizierungsdienst ab, der durch diese Identität beschrieben wird.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.EnterpriseServices.SecurityIdentity.AuthenticationService">
          <source>The authentication service described by this identity.</source>
          <target state="translated">Der Authentifizierungsdienst, der durch diese Identität beschrieben wird.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.EnterpriseServices.SecurityIdentity.ImpersonationLevel">
          <source>Gets the impersonation level of the user described by this identity.</source>
          <target state="translated">Ruft die Ebene des Identitätswechsels für den Benutzer ab, der durch diese Identität beschrieben wird.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.EnterpriseServices.SecurityIdentity.ImpersonationLevel">
