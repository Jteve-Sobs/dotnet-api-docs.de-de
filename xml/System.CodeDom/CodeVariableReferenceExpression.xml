      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source>Represents a reference to a local variable.</source>
          <target state="translated">Stellt einen Verweis auf eine lokale Variable dar.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source><ph id="ph1">&lt;xref:System.CodeDom.CodeVariableReferenceExpression&gt;</ph> can be used to represent a reference to a local variable.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.CodeDom.CodeVariableReferenceExpression&gt;</ph> kann verwendet werden, um einen Verweis auf eine lokale Variable darzustellen.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source>The <ph id="ph1">&lt;xref:System.CodeDom.CodeVariableReferenceExpression.VariableName%2A&gt;</ph> property specifies the name of the local variable to reference.</source>
          <target state="translated">Die <ph id="ph1">&lt;xref:System.CodeDom.CodeVariableReferenceExpression.VariableName%2A&gt;</ph> Eigenschaft gibt den Namen der lokalen Variablen zu verweisen.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source>Use <ph id="ph1">&lt;xref:System.CodeDom.CodeFieldReferenceExpression&gt;</ph> to reference a field.</source>
          <target state="translated">Verwendung <ph id="ph1">&lt;xref:System.CodeDom.CodeFieldReferenceExpression&gt;</ph> auf ein Feld verweisen.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source>Use <ph id="ph1">&lt;xref:System.CodeDom.CodePropertyReferenceExpression&gt;</ph> to reference a property.</source>
          <target state="translated">Verwendung <ph id="ph1">&lt;xref:System.CodeDom.CodePropertyReferenceExpression&gt;</ph> auf eine Eigenschaft verweisen.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source>Use <ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression&gt;</ph> to reference an event.</source>
          <target state="translated">Verwendung <ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression&gt;</ph> auf ein Ereignis verweisen.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source>The following example code demonstrates use of a <ph id="ph1">&lt;xref:System.CodeDom.CodeVariableReferenceExpression&gt;</ph> to refer to a local variable.</source>
          <target state="translated">Der folgende Beispielcode veranschaulicht die Verwendung von einem <ph id="ph1">&lt;xref:System.CodeDom.CodeVariableReferenceExpression&gt;</ph> zum Verweisen auf eine lokale Variable.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="T:System.CodeDom.CodeVariableReferenceExpression">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.CodeDom.CodeVariableReferenceExpression" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.CodeDom.CodeVariableReferenceExpression" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.CodeDom.CodeVariableReferenceExpression.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.CodeDom.CodeVariableReferenceExpression" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.CodeDom.CodeVariableReferenceExpression" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.CodeDom.CodeVariableReferenceExpression.#ctor(System.String)">
          <source>The name of the local variable to reference.</source>
          <target state="translated">Der Name der lokalen Variablen, auf die verwiesen werden soll.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.CodeDom.CodeVariableReferenceExpression.#ctor(System.String)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.CodeDom.CodeVariableReferenceExpression" /&gt;</ph> class using the specified local variable name.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.CodeDom.CodeVariableReferenceExpression" /&gt;</ph>-Klasse unter Verwendung des angegebenen Namens der lokalen Variablen.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.CodeDom.CodeVariableReferenceExpression.VariableName">
          <source>Gets or sets the name of the local variable to reference.</source>
          <target state="translated">Ruft den Namen der lokalen Variablen ab, auf die verwiesen werden soll, oder legt diesen fest.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.CodeDom.CodeVariableReferenceExpression.VariableName">
          <source>The name of the local variable to reference.</source>
          <target state="translated">Der Name der lokalen Variablen, auf die verwiesen werden soll.</target>       </trans-unit>
