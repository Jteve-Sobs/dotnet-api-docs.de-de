      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.RemoveBehaviorElement">
          <source>A configuration element to remove a particular service or endpoint behavior.</source>
          <target state="translated">Ein Konfigurationselement, um das Verhalten eines bestimmten Diensts oder Endpunkts zu entfernen.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.RemoveBehaviorElement.#ctor">
          <source>Creates a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.RemoveBehaviorElement" /&gt;</ph> class.</source>
          <target state="translated">Erstellt eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.RemoveBehaviorElement" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.RemoveBehaviorElement.BehaviorType">
          <source>Gets or sets the type of the behavior that is removed.</source>
          <target state="translated">Ruft den Typ des entfernten Verhaltens ab oder legt ihn fest.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.RemoveBehaviorElement.BehaviorType">
          <source>A behavior type.</source>
          <target state="translated">Ein Verhaltenstyp.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.RemoveBehaviorElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
          <source>The service model extension element that supplies the content.</source>
          <target state="translated">Das Erweiterungselement des Dienstmodells, das den Inhalt angibt.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.RemoveBehaviorElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
          <source>Copies content of the specified <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ServiceModelExtensionElement" /&gt;</ph> to the current <ph id="ph2">&lt;see cref="T:System.ServiceModel.Configuration.RemoveBehaviorElement" /&gt;</ph> object.</source>
          <target state="translated">Kopiert den Inhalt des angegebenen <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.ServiceModelExtensionElement" /&gt;</ph>s in das aktuelle <ph id="ph2">&lt;see cref="T:System.ServiceModel.Configuration.RemoveBehaviorElement" /&gt;</ph>-Objekt.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.RemoveBehaviorElement.Name">
          <source>Gets or sets the name of the configuration element that removes the behavior.</source>
          <target state="translated">Ruft den Namen des Konfigurationselements ab, mit dem das Verhalten entfernt wird, oder legt diesen fest.</target>       </trans-unit>
