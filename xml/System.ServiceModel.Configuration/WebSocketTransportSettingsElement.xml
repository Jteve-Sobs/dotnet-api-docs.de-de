        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.WebSocketTransportSettingsElement">
          <source>Represents WebSocketTransportSettingsElement for WebSocketTransportSettings.</source>
          <target state="translated">Stellt WebSocketTransportSettingsElement für WebSocketTransportSettings dar.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.WebSocketTransportSettingsElement" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.WebSocketTransportSettingsElement" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.ApplyConfiguration(System.ServiceModel.Channels.WebSocketTransportSettings)">
          <source>The web socket transport settings.</source>
          <target state="translated">Die Internet-Sockettransporteinstellungen.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.ApplyConfiguration(System.ServiceModel.Channels.WebSocketTransportSettings)">
          <source>Applies a new configuration for this element.</source>
          <target state="translated">Wendet eine neue Konfiguration für dieses Element an.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.CreateNotificationOnConnection">
          <source>Gets or sets a value that indicates whether to create notification on connection.</source>
          <target state="translated">Ruft einen Wert ab, der angibt, ob bei Verbindung eine Benachrichtigung erstellt werden soll, oder legt diesen fest.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.CreateNotificationOnConnection">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if notification is created; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph>, wenn eine Benachrichtigung erstellt wird; andernfalls <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.DisablePayloadMasking">
          <source>Gets or sets a value that indicates whether to disable payload masking.</source>
          <target state="translated">Ruft einen Wert ab, der angibt, ob die Nutzlastmaske deaktiviert werden soll, oder legt diesen fest.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.DisablePayloadMasking">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> is payload masking is disabled; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph>, wenn die Nutzlastmaske deaktiviert ist; andernfalls <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WebSocketTransportSettingsElement.InitializeFrom(System.ServiceModel.Channels.WebSocketTransportSettings)">
