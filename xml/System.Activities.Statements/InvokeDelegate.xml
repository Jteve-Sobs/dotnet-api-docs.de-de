        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Statements.InvokeDelegate">
          <source>Invokes the specified <ph id="ph1">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph>.</source>
          <target state="translated">Ruft den angegebenen <ph id="ph1">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph> auf.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.InvokeDelegate.#ctor">
          <source>Creates a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Statements.InvokeDelegate" /&gt;</ph> class.</source>
          <target state="translated">Erstellt eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Activities.Statements.InvokeDelegate" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Activities.Statements.InvokeDelegate.Default">
          <source>Gets or sets the default activity when invoke is not required.</source>
          <target state="translated">Ruft die Standardaktivität ab, wenn ein Aufruf nicht erforderlich ist, oder legt diese fest.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Statements.InvokeDelegate.Default">
          <source>The default activity.</source>
          <target state="translated">Die Standardaktivität.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Activities.Statements.InvokeDelegate.Delegate">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph> to invoke.</source>
          <target state="translated">Ruft den aufzurufenden <ph id="ph1">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph> ab.</target>       </trans-unit>
