      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Cryptography.X509Certificates.X509ContentType">
          <source>Specifies the format of an X.509 certificate.</source>
          <target state="translated">Gibt das Format eines X.509-Zertifikats an.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.X509Certificates.X509ContentType">
          <source>Content types Cert, Pkcs12, and SerializedCert can be exported as byte arrays.</source>
          <target state="translated">Inhaltstypen Cert, Pkcs12 und SerializedCert können als Bytearrays exportiert werden.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.X509Certificates.X509ContentType">
          <source>Therefore, all three formats are serialized certificates.</source>
          <target state="translated">Daher sind alle drei Formate serialisierten Zertifikate.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.X509Certificates.X509ContentType">
          <source>SerializedCert differs from an exported Cert file in that it is created by using the <bpt id="p1">[</bpt>CertSerializeCertificateStoreElement function<ept id="p1">](http://go.microsoft.com/fwlink/?LinkId=183293)</ept>, which serializes both the encoded certificate and its encoded properties.</source>
          <target state="translated">SerializedCert unterscheidet sich von einer exportierten Zertifikatsdatei Erstellung mithilfe der <bpt id="p1">[</bpt>CertSerializeCertificateStoreElement-Funktion<ept id="p1">](http://go.microsoft.com/fwlink/?LinkId=183293)</ept>, die das codierte Zertifikat und die codierten Eigenschaften serialisiert.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Cryptography.X509Certificates.X509ContentType">
          <source>If you export the same <ph id="ph1">&lt;xref:System.Security.Cryptography.X509Certificates.X509Certificate&gt;</ph> object in both formats and view the resulting byte arrays, you will see that the two are different.</source>
          <target state="translated">Wenn Sie die gleiche exportieren <ph id="ph1">&lt;xref:System.Security.Cryptography.X509Certificates.X509Certificate&gt;</ph> -Objekt in beiden Formaten und die resultierenden Bytearrays anzeigen, sehen Sie, dass die beiden unterscheiden.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Authenticode">
          <source>An Authenticode X.509 certificate.</source>
          <target state="translated">Ein Authenticode X.509-Zertifikat.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Cert">
          <source>A single X.509 certificate.</source>
          <target state="translated">Ein einzelnes X.509-Zertifikat.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Pfx">
          <source>A PFX-formatted certificate.</source>
          <target state="translated">Ein PFX-formatiertes Zertifikat.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Pfx">
          <source>The <ph id="ph1">&lt;see langword="Pfx" /&gt;</ph> value is identical to the <ph id="ph2">&lt;see langword="Pkcs12" /&gt;</ph> value.</source>
          <target state="translated">Der <ph id="ph1">&lt;see langword="Pfx" /&gt;</ph>-Wert ist identisch mit dem <ph id="ph2">&lt;see langword="Pkcs12" /&gt;</ph>-Wert.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Pkcs12">
          <source>A PKCS #12–formatted certificate.</source>
          <target state="translated">Ein PKCS #12–formatiertes Zertifikat.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Pkcs12">
          <source>The <ph id="ph1">&lt;see langword="Pkcs12" /&gt;</ph> value is identical to the <ph id="ph2">&lt;see langword="Pfx" /&gt;</ph> value.</source>
          <target state="translated">Der <ph id="ph1">&lt;see langword="Pkcs12" /&gt;</ph>-Wert ist identisch mit dem <ph id="ph2">&lt;see langword="Pfx" /&gt;</ph>-Wert.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Pkcs7">
          <source>A PKCS #7–formatted certificate.</source>
          <target state="translated">Ein PKCS #7–formatiertes Zertifikat.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.SerializedCert">
          <source>A single serialized X.509 certificate.</source>
          <target state="translated">Ein einzelnes serialisiertes X.509-Zertifikat.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.SerializedStore">
          <source>A serialized store.</source>
          <target state="translated">Ein serialisierter Speicher.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Security.Cryptography.X509Certificates.X509ContentType.Unknown">
          <source>An unknown X.509 certificate.</source>
          <target state="translated">Ein unbekanntes X.509-Zertifikat.</target>       </trans-unit>
