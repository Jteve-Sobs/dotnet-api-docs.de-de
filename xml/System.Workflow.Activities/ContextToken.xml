          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Workflow.Activities.ContextToken" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.Workflow.Activities.ContextToken.Name">
          <source>Gets or sets the name of this instance.</source>
          <target state="translated">Ruft den Namen dieser Instanz ab oder legt diesen fest.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Workflow.Activities.ContextToken.Name">
          <source>A string that represents the name of this instance.</source>
          <target state="translated">Eine Zeichenfolge, welche den Namen dieser Instanz darstellt.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Workflow.Activities.ContextToken.OwnerActivityName">
          <source>Gets or sets the name of the <ph id="ph1">&lt;see cref="T:System.Workflow.ComponentModel.Activity" /&gt;</ph> associated with this context token.</source>
          <target state="translated">Ruft den Namen der <ph id="ph1">&lt;see cref="T:System.Workflow.ComponentModel.Activity" /&gt;</ph> ab, die diesem Kontexttoken zugeordnet ist, oder legt ihn fest.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Workflow.Activities.ContextToken.OwnerActivityName">
          <source>A string that represents the name of the activity associated with this context token.</source>
          <target state="translated">Eine Zeichenfolge, die den Namen der Aktivität darstellt, die diesem Kontexttoken zugeordnet ist.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Workflow.Activities.ContextToken.RootContextName">
