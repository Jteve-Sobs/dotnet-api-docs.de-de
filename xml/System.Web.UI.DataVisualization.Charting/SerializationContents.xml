      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.DataVisualization.Charting.SerializationContents">
          <source>Specifies the type of chart content to be serialized.</source>
          <target state="translated">Gibt den Typ des zu serialisierenden Diagramminhalts an.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.DataVisualization.Charting.SerializationContents">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.SerializationContents&gt;</ph> enumeration represents the chart content type to be serialized.</source>
          <target state="translated">Die <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.SerializationContents&gt;</ph> -Enumeration stellt dar, den Inhalt Diagrammtyp serialisiert werden soll.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.DataVisualization.Charting.SerializationContents">
          <source>This enumeration is used with the <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.ChartSerializer.Content%2A&gt;</ph> and <ph id="ph2">&lt;xref:System.Web.UI.DataVisualization.Charting.Chart.ViewStateContent%2A&gt;</ph> properties.</source>
          <target state="translated">Diese Enumeration wird von der <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.ChartSerializer.Content%2A&gt;</ph>-Eigenschaft und der <ph id="ph2">&lt;xref:System.Web.UI.DataVisualization.Charting.Chart.ViewStateContent%2A&gt;</ph>-Eigenschaft verwendet.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.SerializationContents.All">
          <source>Serialize all chart information.</source>
          <target state="translated">Serialisiert alle Diagramminformationen.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.SerializationContents.All">
          <source>This includes all appearance properties and data point values.</source>
          <target state="translated">Dies umfasst alle Darstellungseigenschaften und Datenpunktwerte.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.SerializationContents.Appearance">
          <source>Serialize all appearance properties, such as color or line style, if they have non-default values</source>
          <target state="translated">Serialisiert alle Darstellungseigenschaften, z. B. Farbe oder Linienart, wenn sie über keine Standardwerte verfügen.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.SerializationContents.Data">
          <source>Serialize data values from all chart series.</source>
          <target state="translated">Serialisiert Datenwerte aus allen Diagrammreihen.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.SerializationContents.Data">
          <source>Does not include appearance properties.</source>
          <target state="translated">Umfasst keine Darstellungseigenschaften.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.SerializationContents.Default">
          <source>Serialize all non-default chart content.</source>
          <target state="translated">Serialisiert den gesamten nicht standardmäßigen Diagramminhalt.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.SerializationContents.Default">
          <source>This includes appearance properties, series and their data points, axis minimums/maximums, and so forth.</source>
          <target state="translated">Umfasst Darstellungseigenschaften, Reihen und deren Datenpunkte, Achsenhöchst-/mindestwerte usw.</target>       </trans-unit>
