        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.JScript.CmdLineError">
          <source>Specifies errors that occur when you run the command-line compiler jsc.exe.</source>
          <target state="translated">Gibt Fehler an, die beim Ausführen des Befehlszeilencompilers jsc.exe auftreten.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:Microsoft.JScript.CmdLineError">
          <source>This enumeration is used to create <ph id="ph1">&lt;see cref="T:Microsoft.JScript.CmdLineException" /&gt;</ph> objects.</source>
          <target state="translated">Diese Enumeration wird zum Erstellen von <ph id="ph1">&lt;see cref="T:Microsoft.JScript.CmdLineException" /&gt;</ph>-Objekten verwendet.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:Microsoft.JScript.CmdLineError.AssemblyNotFound">
          <source>The referenced assembly '{0}' could not be found.</source>
          <target state="translated">Die Assembly {0}, auf die verwiesen wird, konnte nicht gefunden werden.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:Microsoft.JScript.CmdLineError.CannotCreateEngine">
