      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.JScript.SimpleHashtable">
          <source>A hash table for the internal storage of items.</source>
          <target state="translated">Eine Hashtabelle für den internen Speicher von Elementen.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.SimpleHashtable.#ctor(System.UInt32)">
          <source>The number of items at which the hash table must be resized.</source>
          <target state="translated">Die Anzahl der Elemente, bei der die Größe der Hashtabelle geändert werden muss.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.SimpleHashtable.#ctor(System.UInt32)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:Microsoft.JScript.SimpleHashtable" /&gt;</ph> class, using the specified threshold.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:Microsoft.JScript.SimpleHashtable" /&gt;</ph>-Klasse unter Verwendung des angegebenen Schwellenwerts.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.SimpleHashtable.GetEnumerator">
          <source>Gets an enumerator for the hash table.</source>
          <target state="translated">Ruft einen Enumerator für die Hashtabelle ab.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.SimpleHashtable.GetEnumerator">
          <source>An enumerator for the hash table.</source>
          <target state="translated">Ein Enumerator für die Hashtabelle.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:Microsoft.JScript.SimpleHashtable.Item(System.Object)">
          <source>The key of the item to get or set.</source>
          <target state="translated">Der Schlüssel des abzurufenden oder festzulegenden Elements.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:Microsoft.JScript.SimpleHashtable.Item(System.Object)">
          <source>Gets or sets the item that has the specified key.</source>
          <target state="translated">Ruft das Element mit dem angegebenen Schlüssel ab oder legt dieses fest.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.JScript.SimpleHashtable.Item(System.Object)">
          <source>The value of the item that has the specified <ph id="ph1">&lt;paramref name="key" /&gt;</ph>.</source>
          <target state="translated">Der Wert des Elements mit dem angegebenen <ph id="ph1">&lt;paramref name="key" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.SimpleHashtable.Remove(System.Object)">
          <source>The key of the item to remove.</source>
          <target state="translated">Der Schlüssel des zu entfernenden Elements.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:Microsoft.JScript.SimpleHashtable.Remove(System.Object)">
          <source>Removes the item with the specified key from the hash table.</source>
          <target state="translated">Entfernt das Element mit dem angegebenen Schlüssel aus der Hashtabelle.</target>       </trans-unit>
