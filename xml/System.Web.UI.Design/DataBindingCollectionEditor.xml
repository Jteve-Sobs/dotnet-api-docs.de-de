      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.Design.DataBindingCollectionEditor">
          <source>Provides a user interface for editing a collection of data bindings.</source>
          <target state="translated">Stellt eine Benutzeroberfläche zum Bearbeiten einer Auflistung von Datenbindungen bereit.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.Design.DataBindingCollectionEditor">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.Design.DataBindingCollectionEditor&gt;</ph> class is obsolete.</source>
          <target state="translated">Die <ph id="ph1">&lt;xref:System.Web.UI.Design.DataBindingCollectionEditor&gt;</ph> Klasse ist veraltet.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.Design.DataBindingCollectionEditor">
          <source>At design time, editing of data bindings is accomplished from a <ph id="ph1">&lt;xref:System.ComponentModel.Design.DesignerActionList&gt;</ph> instead of the property grid.</source>
          <target state="translated">Zur Entwurfszeit, Bearbeitung von datenbindungen erfolgt aus einem <ph id="ph1">&lt;xref:System.ComponentModel.Design.DesignerActionList&gt;</ph> anstatt über das Eigenschaftenraster.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.Design.DataBindingCollectionEditor">
          <source>For more information, see <ph id="ph1">&lt;xref:System.Web.UI.Design.ControlDesigner.ActionLists%2A&gt;</ph>.</source>
          <target state="translated">Weitere Informationen finden Sie unter <ph id="ph1">&lt;xref:System.Web.UI.Design.ControlDesigner.ActionLists%2A&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.DataBindingCollectionEditor" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.DataBindingCollectionEditor" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>An <ph id="ph1">&lt;see cref="T:System.ComponentModel.ITypeDescriptorContext" /&gt;</ph> object that identifies the component or control the collection belongs to.</source>
          <target state="translated">Ein <ph id="ph1">&lt;see cref="T:System.ComponentModel.ITypeDescriptorContext" /&gt;</ph>-Objekt, das die Komponente oder das Steuerelement angibt, zu der bzw. dem die Auflistung gehört.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>The <ph id="ph1">&lt;see cref="T:System.IServiceProvider" /&gt;</ph> to use.</source>
          <target state="translated">Der zu verwendende <ph id="ph1">&lt;see cref="T:System.IServiceProvider" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>The collection to edit.</source>
          <target state="translated">Die zu bearbeitende Auflistung.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>Edits the value of the specified data-binding collection using the specified service provider and context.</source>
          <target state="translated">Bearbeitet den Wert der angegebenen Datenbindungsauflistung, wobei der angegebene Dienstanbieter und der angegebene Kontext verwendet werden.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)">
          <source>The new collection.</source>
          <target state="translated">Die neue Auflistung.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>An <ph id="ph1">&lt;see cref="T:System.ComponentModel.ITypeDescriptorContext" /&gt;</ph> object that identifies the component or control to retrieve the edit style for.</source>
          <target state="translated">Ein <ph id="ph1">&lt;see cref="T:System.ComponentModel.ITypeDescriptorContext" /&gt;</ph>-Objekt, das die Komponente oder das Steuerelement angibt, deren bzw. dessen Bearbeitungsstil abgerufen werden soll.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>Gets the editor style used by the <ph id="ph1">&lt;see cref="M:System.Web.UI.Design.DataBindingCollectionEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)" /&gt;</ph> method.</source>
          <target state="translated">Ruft den von der <ph id="ph1">&lt;see cref="M:System.Web.UI.Design.DataBindingCollectionEditor.EditValue(System.ComponentModel.ITypeDescriptorContext,System.IServiceProvider,System.Object)" /&gt;</ph>-Methode verwendeten Editor-Stil ab.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>A <ph id="ph1">&lt;see cref="T:System.Drawing.Design.UITypeEditorEditStyle" /&gt;</ph> object that specifies the editor style of the component or control.</source>
          <target state="translated">Ein <ph id="ph1">&lt;see cref="T:System.Drawing.Design.UITypeEditorEditStyle" /&gt;</ph>-Objekt, das den Editor-Stil der Komponente oder des Steuerelements angibt.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="M:System.Web.UI.Design.DataBindingCollectionEditor.GetEditStyle(System.ComponentModel.ITypeDescriptorContext)">
          <source>This editor provides a modal editor style.</source>
          <target state="translated">Dieser Editor stellt einen modalen Editor-Stil.</target>       </trans-unit>
      </group>
