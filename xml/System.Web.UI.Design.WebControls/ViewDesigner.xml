      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.Design.WebControls.ViewDesigner">
          <source>Provides design-time support in a visual designer for the <ph id="ph1">&lt;see cref="T:System.Web.UI.WebControls.View" /&gt;</ph> control.</source>
          <target state="translated">Stellt Entwurfszeitunterstützung in einem visuellen Designer für das <ph id="ph1">&lt;see cref="T:System.Web.UI.WebControls.View" /&gt;</ph>-Steuerelement bereit.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.ViewDesigner.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.ViewDesigner" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.ViewDesigner" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="T:System.Web.UI.Design.WebControls.ViewDesigner">
          <source>Retrieves the HTML markup that is used to represent the control at design time.</source>
          <target state="translated">Ruft das HTML-Markup ab, das für die Darstellung des Steuerelements zur Entwurfszeit verwendet wird.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.ViewDesigner.GetDesignTimeHtml">
          <source>Retrieves the HTML markup that is used to represent the control at design time.</source>
          <target state="translated">Ruft das HTML-Markup ab, das für die Darstellung des Steuerelements zur Entwurfszeit verwendet wird.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.ViewDesigner.GetDesignTimeHtml">
          <source>The HTML markup used to represent the control at design time.</source>
          <target state="translated">Das HTML-Markup, das für die Darstellung des Steuerelements zur Entwurfszeit verwendet wird.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.ViewDesigner.GetDesignTimeHtml(System.Web.UI.Design.DesignerRegionCollection)">
          <source>A collection of designer regions.</source>
          <target state="translated">Eine Auflistung von Designerbereichen.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.ViewDesigner.GetDesignTimeHtml(System.Web.UI.Design.DesignerRegionCollection)">
          <source>Retrieves the HTML markup to display the control and populates the collection with the current control designer regions.</source>
          <target state="translated">Ruft das HTML-Markup zum Anzeigen des Steuerelements ab und füllt die Auflistung mit den aktuellen Steuerelement-Designer-Bereichen auf.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.ViewDesigner.GetDesignTimeHtml(System.Web.UI.Design.DesignerRegionCollection)">
          <source>An HTML markup string that represents the control.</source>
          <target state="translated">Eine HTML-Markup-Zeichenfolge, die das Steuerelement darstellt.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.ViewDesigner.Initialize(System.ComponentModel.IComponent)">
          <source>An <ph id="ph1">&lt;see cref="T:System.ComponentModel.IComponent" /&gt;</ph> object.</source>
          <target state="translated">Ein <ph id="ph1">&lt;see cref="T:System.ComponentModel.IComponent" /&gt;</ph>-Objekt.</target>       </trans-unit>
