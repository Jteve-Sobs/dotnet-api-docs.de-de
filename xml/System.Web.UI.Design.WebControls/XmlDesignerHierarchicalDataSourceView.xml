        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView">
          <source>Provides a hierarchical design-time view of data for the <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.XmlDataSourceDesigner" /&gt;</ph> class.</source>
          <target state="translated">Stellt eine hierarchische Entwurfszeitansicht von Daten für die <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.XmlDataSourceDesigner" /&gt;</ph>-Klasse bereit.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView.#ctor(System.Web.UI.Design.WebControls.XmlDataSourceDesigner,System.String)">
          <source>An <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.XmlDataSourceDesigner" /&gt;</ph>.</source>
          <target state="translated">Eine <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.XmlDataSourceDesigner" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView.#ctor(System.Web.UI.Design.WebControls.XmlDataSourceDesigner,System.String)">
          <source>An XPath string that identifies the data for the view.</source>
          <target state="translated">Eine XPath-Zeichenfolge, die die Daten für die Ansicht angibt.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView.#ctor(System.Web.UI.Design.WebControls.XmlDataSourceDesigner,System.String)">
          <source>Creates a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView" /&gt;</ph> class using the provided designer and XPath.</source>
          <target state="translated">Erstellt eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView" /&gt;</ph>-Klasse mit dem bereitgestellten Designer und XPath.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView.GetDesignTimeData(System.Boolean@)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> to indicate that the returned data is sample data; <ph id="ph2">&lt;see langword="false" /&gt;</ph> to indicate that the returned data is live data.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph>, um anzugeben, dass die zurückgegebenen Daten Beispieldaten sind, <ph id="ph2">&lt;see langword="false" /&gt;</ph>, um anzugeben, dass es sich bei den zurückgegebenen Daten um Livedaten handelt.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView.GetDesignTimeData(System.Boolean@)">
          <source>Generates design-time data that matches the schema of the associated data source control and returns a Boolean value indicating whether the data is sample or real data.</source>
          <target state="translated">Generiert Entwurfszeitdaten, die dem Schema des zugeordneten Datenquellen-Steuerelements entsprechen, und gibt einen booleschen Wert zurück, der angibt, ob es sich um Beispieldaten oder echte Daten handelt.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView.GetDesignTimeData(System.Boolean@)">
          <source>This implementation always returns <ph id="ph1">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated">Diese Implementierung gibt immer <ph id="ph1">&lt;see langword="null" /&gt;</ph> zurück.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Web.UI.Design.WebControls.XmlDesignerHierarchicalDataSourceView.Schema">
          <source>Gets a schema that describes the data source view that is represented by this view object.</source>
          <target state="translated">Ruft ein Schema ab, das die Datenquellensicht beschreibt, die von diesem Ansichtsobjekt dargestellt wird.</target>       </trans-unit>
