        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.Design.WebControls.SiteMapPathDesigner">
          <source>Provides design-time support in a visual designer for the <ph id="ph1">&lt;see cref="T:System.Web.UI.WebControls.SiteMapPath" /&gt;</ph> control.</source>
          <target state="translated">Stellt Entwurfszeitunterstützung in einem visuellen Designer für das <ph id="ph1">&lt;see cref="T:System.Web.UI.WebControls.SiteMapPath" /&gt;</ph>-Steuerelement bereit.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapPathDesigner.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.SiteMapPathDesigner" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.WebControls.SiteMapPathDesigner" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.Web.UI.Design.WebControls.SiteMapPathDesigner.AutoFormats">
          <source>Gets the collection of automatic formatting schemes to display in the <bpt id="p1">**</bpt>Auto Format<ept id="p1">**</ept> dialog box for the associated control at design time.</source>
          <target state="translated">Ruft die Auflistung der Schemas für die automatische Formatierung ab, die zur Entwurfszeit für das zugeordnete Steuerelement im Dialogfeld <bpt id="p1">**</bpt>Automat. Formatierung<ept id="p1">**</ept> angezeigt werden sollen.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Web.UI.Design.WebControls.SiteMapPathDesigner.AutoFormats">
          <source>Returns <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.DesignerAutoFormatCollection" /&gt;</ph>.</source>
          <target state="translated">Gibt <ph id="ph1">&lt;see cref="T:System.Web.UI.Design.DesignerAutoFormatCollection" /&gt;</ph>zurück.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.UI.Design.WebControls.SiteMapPathDesigner.GetDesignTimeHtml">
          <source>Gets the HTML markup that is used to represent the control at design time.</source>
          <target state="translated">Ruft das HTML-Markup ab, das für die Darstellung des Steuerelements zur Entwurfszeit verwendet wird.</target>       </trans-unit>
