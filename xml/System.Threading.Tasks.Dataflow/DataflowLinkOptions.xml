          <source>PropagateCompletion</source>
          <target state="translated">PropagateCompletion</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Threading.Tasks.Dataflow.DataflowLinkOptions">
          <source>False</source>
          <target state="translated">False</target>       </trans-unit>
