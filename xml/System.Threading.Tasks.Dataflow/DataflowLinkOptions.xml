        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Threading.Tasks.Dataflow.DataflowLinkOptions">
          <source>Dataflow blocks capture the state of the options at linking.</source>
          <target state="translated">Datenflussblöcke erfassen den Zustand der Optionen auf verknüpfen.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Threading.Tasks.Dataflow.DataflowLinkOptions">
          <source>Subsequent changes to the provided  <ph id="ph1">&lt;xref:System.Threading.Tasks.Dataflow.DataflowLinkOptions&gt;</ph> instance should not affect the behavior of a link.</source>
          <target state="translated">Nachfolgende Änderungen an der bereitgestellten <ph id="ph1">&lt;xref:System.Threading.Tasks.Dataflow.DataflowLinkOptions&gt;</ph> Instanz sollte keinen Einfluss auf das Verhalten einer Verknüpfung.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Threading.Tasks.Dataflow.DataflowLinkOptions.#ctor">
          <source>Initializes the <ph id="ph1">&lt;see cref="T:System.Threading.Tasks.Dataflow.DataflowLinkOptions" /&gt;</ph>.</source>
          <target state="translated">Initialisiert das <ph id="ph1">&lt;see cref="T:System.Threading.Tasks.Dataflow.DataflowLinkOptions" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:System.Threading.Tasks.Dataflow.DataflowLinkOptions.Append">
          <source>Gets or sets whether the link should be appended to the source’s list of links, or whether it should be prepended.</source>
          <target state="translated">Ruft ab oder legt fest, ob der Link der Linkliste der Quelle angefügt oder ob er vorangestellt werden soll.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Threading.Tasks.Dataflow.DataflowLinkOptions.Append">
          <source>Returns  <ph id="ph1">&lt;see cref="T:System.Boolean" /&gt;</ph>.</source>
          <target state="translated">Gibt <ph id="ph1">&lt;see cref="T:System.Boolean" /&gt;</ph> zurück.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="P:System.Threading.Tasks.Dataflow.DataflowLinkOptions.MaxMessages">
          <source>Gets or sets the maximum number of messages that may be consumed across the link.</source>
          <target state="translated">Ruft die maximale Anzahl an Nachrichten ab, die über den Link verbraucht werden können, oder legt diese fest.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Threading.Tasks.Dataflow.DataflowLinkOptions.MaxMessages">
          <source>Returns  <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph>.</source>
          <target state="translated">Gibt <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph> zurück.</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="P:System.Threading.Tasks.Dataflow.DataflowLinkOptions.PropagateCompletion">
          <source>Gets or sets whether the linked target will have completion and faulting notification propagated to it automatically.</source>
          <target state="translated">Ruft ab oder legt fest, ob an das verknüpfte Ziel automatisch eine Abschluss- oder Fehlerbenachrichtigung weitergegeben wird.</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Threading.Tasks.Dataflow.DataflowLinkOptions.PropagateCompletion">
          <source>Returns  <ph id="ph1">&lt;see cref="T:System.Boolean" /&gt;</ph>.</source>
          <target state="translated">Gibt <ph id="ph1">&lt;see cref="T:System.Boolean" /&gt;</ph> zurück.</target>       </trans-unit>
