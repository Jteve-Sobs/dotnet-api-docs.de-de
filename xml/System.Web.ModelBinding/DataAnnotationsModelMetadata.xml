      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.ModelBinding.DataAnnotationsModelMetadata">
          <source>Provides a container for common metadata, for the <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelMetadataProvider" /&gt;</ph> class, and for the <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelValidator" /&gt;</ph> class for a data model.</source>
          <target state="translated">Stellt einen Container für allgemeine Metadaten für die <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelMetadataProvider" /&gt;</ph>-Klasse und für die <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelValidator" /&gt;</ph>-Klasse für ein Datenmodell bereit.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The provider object.</source>
          <target state="translated">Das Anbieterobjekt.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The type of the container, or <ph id="ph1">&lt;see langword="null" /&gt;</ph> if there is no container.</source>
          <target state="translated">Der Typ des Containers oder <ph id="ph1">&lt;see langword="null" /&gt;</ph>, wenn kein Container vorhanden sind.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The model accessor.</source>
          <target state="translated">Der Modellaccessor.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The type of the model.</source>
          <target state="translated">Der Typ des Modells.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The name of the property, or <ph id="ph1">&lt;see langword="null" /&gt;</ph> if the model is not a property.</source>
          <target state="translated">Der Name der Eigenschaft oder <ph id="ph1">&lt;see langword="null" /&gt;</ph>, wenn das Modell keine Eigenschaft ist.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>The display column attribute.</source>
          <target state="translated">Das Attribut der Anzeigespalte.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.#ctor(System.Web.ModelBinding.DataAnnotationsModelMetadataProvider,System.Type,System.Func{System.Object},System.Type,System.String,System.ComponentModel.DataAnnotations.DisplayColumnAttribute)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelMetadata" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:System.Web.ModelBinding.DataAnnotationsModelMetadata" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.GetSimpleDisplayText">
          <source>Returns text to display for the model when the model represents a complex object.</source>
          <target state="translated">Gibt einen Text zurück, der für das Modell angezeigt wird, wenn dieses ein komplexes Objekt darstellt.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.GetSimpleDisplayText">
          <source>The text to display for a complex object.</source>
          <target state="translated">Der Text, der für ein komplexes Objekt angezeigt werden soll.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.GetSimpleDisplayText">
          <source>If there is a <ph id="ph1">&lt;see cref="T:System.ComponentModel.DataAnnotations.DisplayColumnAttribute" /&gt;</ph> attribute for the model, the default is the name of the display column.</source>
          <target state="translated">Wenn ein <ph id="ph1">&lt;see cref="T:System.ComponentModel.DataAnnotations.DisplayColumnAttribute" /&gt;</ph>-Attribut für das Modell vorhanden ist, wird standardmäßig der Name der Anzeigespalte angegeben.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Web.ModelBinding.DataAnnotationsModelMetadata.GetSimpleDisplayText">
          <source>Otherwise the default is determined by calling the <ph id="ph1">&lt;see cref="M:System.Web.ModelBinding.ModelMetadata.GetSimpleDisplayText" /&gt;</ph> method of the <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.ModelMetadata" /&gt;</ph> base class.</source>
          <target state="translated">Andernfalls wird der Standardwert bestimmt, indem die <ph id="ph1">&lt;see cref="M:System.Web.ModelBinding.ModelMetadata.GetSimpleDisplayText" /&gt;</ph>-Methode der <ph id="ph2">&lt;see cref="T:System.Web.ModelBinding.ModelMetadata" /&gt;</ph>-Basisklasse aufrufen wird.</target>       </trans-unit>
