        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Tasks.CodeTaskFactory">
          <source>A task factory which can take a code DOM supported language and create a task from it.</source>
          <target state="translated">Eine Aufgabenfactory, die eine Code-DOM-unterstützte Sprache akzeptiert und aus dieser eine Aufgabe erstellen kann.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.CodeTaskFactory.#ctor">
          <source>Creates the task factory.</source>
          <target state="translated">Erstellt die Aufgabenfactory.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.CodeTaskFactory.CleanupTask(Microsoft.Build.Framework.ITask)">
          <source>The task to clean up.</source>
          <target state="translated">Die zu bereinigende Aufgabe.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.CodeTaskFactory.CleanupTask(Microsoft.Build.Framework.ITask)">
          <source>Cleans up any context or state used in a task.</source>
          <target state="translated">Bereinigt alle Kontexte oder Zustände in einer Aufgabe.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.CodeTaskFactory.CreateTask(Microsoft.Build.Framework.IBuildEngine)">
