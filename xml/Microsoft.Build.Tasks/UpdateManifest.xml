        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Tasks.UpdateManifest">
          <source>Updates selected properties in a manifest and resigns.</source>
          <target state="translated">Aktualisiert die ausgewählten Eigenschaften in einem Manifest und führt das Signieren erneut aus.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.UpdateManifest.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:Microsoft.Build.Tasks.UpdateManifest" /&gt;</ph> class.</source>
          <target state="translated">Initialisiert eine neue Instanz der <ph id="ph1">&lt;see cref="T:Microsoft.Build.Tasks.UpdateManifest" /&gt;</ph>-Klasse.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.UpdateManifest.ApplicationManifest">
          <source>Gets or sets the application manifest.</source>
          <target state="translated">Ruft das Anwendungsmanifest ab oder legt es fest.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.UpdateManifest.ApplicationManifest">
